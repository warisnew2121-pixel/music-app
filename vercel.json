{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "resourses/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/resourses/(.*)",
      "dest": "/resourses/$1"
    },
    {
      "src": "/(.*)\\.(css|js)$",
      "dest": "/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}