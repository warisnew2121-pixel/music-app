{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(styles\\.css|responsive\\.css|utility\\.css|src\\.js)",
      "dest": "/$1"
    },
    {
      "src": "/resourses/(.*)",
      "dest": "/resourses/$1"
    },
    {
      "src": "/songs/(.*)",
      "dest": "/songs/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}