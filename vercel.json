{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(styles|responsive|utility)\\.css",
      "dest": "/$1.css"
    },
    {
      "src": "/src\\.js",
      "dest": "/src.js"
    },
    {
      "src": "/resourses/(.*)",
      "dest": "/resourses/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
