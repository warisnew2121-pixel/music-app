{
  "version": 2,
  "rewrites": [
    {
      "source": "/(styles\\.css|responsive\\.css|utility\\.css|src\\.js)",
      "destination": "/$1"
    },
    {
      "source": "/resourses/(.*)",
      "destination": "/resourses/$1"
    },
    {
      "source": "/songs/(.*)",
      "destination": "/songs/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/server.js"
    }
  ]
}